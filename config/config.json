{
  "title": "Архив стримов BlackUFA",
  "domain": "blackufa.thedrhax.pw",
  "prefix": "https://blackufa.thedrhax.pw",
  "dev_prefix": "http://localhost:8000",
  "github": {
    "user": "TheDrHax",
    "repo": "BlackSilverUfa"
  },
  "versions": {
    "data": {
      "major": 1,
      "minor": 0
    }
  },
  "repos": {
    "remotes": [
      {
        "name": "data",
        "pull": "https://github.com/BlackSilverUfa/data.git",
        "push": "git@github.com:BlackSilverUfa/data.git"
      },
      {
        "name": "chats-generated",
        "pull": "https://github.com/BlackSilverUfa/chats-generated.git",
        "push": "git@github.com:BlackSilverUfa/chats-generated.git"
      },
      {
        "name": "chats-2017",
        "pull": "https://github.com/BlackSilverUfa/chats-2017.git",
        "push": "git@github.com:BlackSilverUfa/chats-2017.git"
      },
      {
        "name": "chats-2018",
        "pull": "https://github.com/BlackSilverUfa/chats-2018.git",
        "push": "git@github.com:BlackSilverUfa/chats-2018.git"
      },
      {
        "name": "chats-2019",
        "pull": "https://github.com/BlackSilverUfa/chats-2019.git",
        "push": "git@github.com:BlackSilverUfa/chats-2019.git"
      },
      {
        "name": "chats-2020",
        "pull": "https://github.com/BlackSilverUfa/chats-2020.git",
        "push": "git@github.com:BlackSilverUfa/chats-2020.git"
      },
      {
        "name": "chats-2021",
        "pull": "https://github.com/BlackSilverUfa/chats-2021.git",
        "push": "git@github.com:BlackSilverUfa/chats-2021.git"
      }
    ],
    "mounts": {
      "data": {
        "remote": "data",
        "branch": "master",
        "prefix": null
      },
      "$PREFIX": {
        "remote": "origin",
        "branch": "gh-pages",
        "prefix": "https://blackufa-chats.thedrhax.pw"
      },
      "$PREFIX/chats/generated": {
        "remote": "chats-generated",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-generated"
      },
      "$PREFIX/chats/2017": {
        "remote": "chats-2017",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-2017"
      },
      "$PREFIX/chats/2018": {
        "remote": "chats-2018",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-2018"
      },
      "$PREFIX/chats/2019": {
        "remote": "chats-2019",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-2019"
      },
      "$PREFIX/chats/2020": {
        "remote": "chats-2020",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-2020"
      },
      "$PREFIX/chats/2021": {
        "remote": "chats-2021",
        "branch": "master",
        "prefix": "https://blackufa-chats.thedrhax.pw/chats-2021"
      }
    }
  },
  "fallback": {
    "prefix": "https://bsufiles.thedrhax.pw/streams",
    "directory": "/media/BlackUFA/Streams",
    "capacity": 50,
    "parse_index": true,
    "streams": true,
    "chats": true,
    "redirects": true,
    "torrents": true
  },
  "blacklist": {
    "users": [
      "nightbot",
      "streamelements",
      "dudelkabot",
      "thedrhaxbot"
    ],
    "messages": [
      "^!.*",
      "^\\\\.*"
    ]
  }
}
