      "$PREFIX/chats/2023": {
        "remote": "chats-2023",
        "branch": "master",
        "prefix": "https://cdn.jsdelivr.net/gh/BlackSilverUfa/chats-2023@master"
      }
    }
  },
  "fallback": {
    "prefix": "https://bsufiles.drhx.ru/streams",
    "hls_proxy_suffix": "/master.m3u8",
    "directory": "/media/BlackUFA/Streams",
    "capacity": 50,
    "parse_index": true,
    "streams": true,
    "chats": true,
    "redirects": true,
    "torrents": true
  },
  "blacklist": {
    "users": [
      "nightbot",
      "streamelements",
      "dudelkabot",
      "thedrhaxbot"
    ],
    "messages": [
      "^!.*",
      "^\\\\.*",
      "^Ту-туууу Hype.*"
    ]
  },
  "chapters": [
    ["^(Стримлер|Артур)", "first", "end", "name: Стримлер"],
    ["^Интро", "first", "after: Стримлер", "name: Игра"],
    ["^(Заставка|Меню|Лобби|Игра|Поиски коопа)", "first", "name: Игра"],
    ["^Арты"],
    ["^[0-9]+-й стрим"],
    ["^Проблемы с правообладателями", "exclude"],
    ["^Боссы", "exclude"]
  ]
}
